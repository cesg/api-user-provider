{
    "name": "cesg/api-user-provider",
    "description": "Laravel UserProvider over rest api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CristianSG",
            "email": "cesgbla@gmail.com"
        }
